#include<iostream>
#include<vector>

using namespace std;

int main(){
    int N, v, cnt=0;
    vector<int> vec;
    cin >> N;
    
    for(int i=0; i<N; i++){
        int t;
        cin >> t;
        vec.push_back(t);
    }
    cin >> v;
    
    for(int i=0; i<N; i++){
        if(vec[i] == v) cnt++;
    }
    cout << cnt;
    return 0;
}
